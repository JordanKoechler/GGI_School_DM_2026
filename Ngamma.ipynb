   "source": [
    "from CosmiXs import annihilation_spectrum\n",
    "from scipy.integrate import simpson\n",
    "\n",
    "spec = annihilation_spectrum(50, 'b', 'Gamma', path='CosmiXs')\n",
    "\n",
    "mask = (spec[:, 0] > 0.5) & (spec[:, 0] < 5) # 0.5 to 5 GeV\n",
    "simpson(y=spec[:, 1][mask], x=spec[:, 0][mask])"
   ]
  }
